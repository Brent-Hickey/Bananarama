curl -X "POST" "http://localhost:18081/realms/com.magenta.public/clients" \
     -H "Content-Type: application/json; charset=utf-8" \
     -H "Accept: application/json; charset=utf-8" \
     -d $'{ "client_id": "1234", "client_secret": "4567", "description": "A test client" }'

curl -X "DELETE" "http://localhost:18081/realms/com.magenta.public/security_enabled" \
     -H "Content-Type: application/json; charset=utf-8" \
     -H "Accept: application/json; charset=utf-8"

curl -X "POST" "http://localhost:18081/realms/com.magenta.public/resource_owners" \
     -H "Content-Type: application/json; charset=utf-8" \
     -H "Accept: application/json; charset=utf-8" \
     -d $'{ "username": "rako", "password": "12345", "meta" :{ "user_id": 3, "account_id": 1 } }'

# Cargar api (meter en ci de api)
curl -X "POST" "http://kube.leapsight.io:32501/services/load_api_spec" \
     -H "Content-Type: application/json; charset=utf-8" \
     -H "Accept: application/json; charset=utf-8" \
	 --data-binary "@magenta_api.bondy.json"